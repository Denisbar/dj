{
  "skinName": "default",
  "title": "#WDC Datasets Exploration",
  "description": "Exploration of WDC Datasets is the single page application. You can use it for search and retrieve WDC data.",
  "keywords": [
    "#Data Management",
    "#World Data System",
    "#Sustainable Development"
  ],
  "isPublished": true,
  "appWidgets": [
    {
      "type": "language-selector",
      "instanceName": "language-selector",
      "showFlags": true
    },
    {
      "type": "page-list",
      "instanceName": "page-list-nav"
    },
    {
      "logoURL": "http://localhost:8080/./api/resource/dm-dm-icon.png",
      "type": "app-logo",
      "instanceName": "app-logo-widget"
    }
  ],
  "pages": [
    {
      "href": "404",
      "template": "1-col",
      "holders": {
        "column": {
          "widgets": [
            {
              "type": "title",
              "title": "404 error",
              "instanceName": "title"
            },
            {
              "type": "htmlwidget",
              "text": "Page not found",
              "instanceName": "error-message"
            }
          ]
        }
      }
    },
    {
      "shortTitle": "#Home",
      "href": "",
      "template": "2(3x9)-cols",
      "holders": {
        "column": {
          "widgets": [
            {
              "rlisteners": "sources,authors,keywords,topics,search,indicators",
              "icon": "/widgets/dm-dataset-manager/icon.png",
              "type": "dm-dataset-manager",
              "instanceName": "Datasets Management"
            }
          ],
          "width": 1125
        },
        "topleft": {
          "widgets": [
            {
              "title": "#Topics",
              "lookupListeners": "search",
              "searchListeners": "search",
              "icon_class": "fi-folder",
              "icon": "/widgets/dm-tag-tree/icon.png",
              "type": "dm-tag-tree",
              "instanceName": "topics"
            },
            {
              "title": "#Indicators",
              "property": "metadata.dimension.concept.values.*.label",
              "lookupListeners": "search",
              "searchListeners": "search",
              "icon_class": "fi-graph-trend",
              "icon": "/widgets/dm-tag-list/icon.png",
              "type": "dm-tag-list",
              "instanceName": "indicators"
            },
            {
              "title": "#Sources",
              "property": "metadata.dataset.source",
              "lookupListeners": "search",
              "searchListeners": "search",
              "icon_class": "fi-web",
              "icon": "/widgets/dm-tag-list/icon.png",
              "type": "dm-tag-list",
              "instanceName": "sources"
            },
            {
              "title": "#Authors",
              "property": "metadata.dataset.commit.author",
              "lookupListeners": "search",
              "searchListeners": "search",
              "icon_class": "fi-torsos-all",
              "icon": "/widgets/dm-tag-list/icon.png",
              "type": "dm-tag-list",
              "instanceName": "authors"
            },
            {
              "title": "#Keywords",
              "property": "metadata.dataset.topics",
              "lookupListeners": "search",
              "searchListeners": "search",
              "icon_class": "fi-key",
              "icon": "/widgets/dm-tag-list/icon.png",
              "type": "dm-tag-list",
              "instanceName": "keywords"
            }
          ],
          "width": 269
        },
        "topright": {
          "widgets": [
            {
              "title": "#Search_Result",
              "query": "",
              "listeners": "search",
              "rlisteners": "sources,authors,keywords,topics,search,indicators",
              "icon_class": "fi-page-search",
              "icon": "/widgets/dm-search-result/icon.png",
              "type": "dm-search-result",
              "instanceName": "search"
            },
            {
              "url": "#url",
              "languageSelector": "language-selector",
              "masterWidget": "search",
              "type": "remote-html",
              "instanceName": "DEFAULT VIEW",
              "icon": "/widgets/remote-html/icon.png"
            }
          ],
          "width": 846
        },
        "bottom": {
          "widgets": [],
          "width": 1125
        }
      },
