{
  "chart": {
    "type": "timelineChart",
    "height": 300,
    "showNavigator": true,
   

    "margin": {
      "top": 20,
      "right": 40,
      "bottom": 40,
      "left": 20
    },
    
 "legend": {
      "dispatch": {},
      "margin": {
        "top": 5,
        "right": 0,
        "bottom": 5,
        "left": 0
      },
      "align": true,
      "rightAlign": true,
      "updateState": true,
      "radioButtonMode": false
    }
  },
  "title": {
    "enable": true,
    "text": "Write Your Title",
    "class": "chartTitle",
    "css": {
      "width": "nullpx",
      "textAlign": "center"
    }
  },
  "subtitle": {
    "enable": true,
    "text": "Write Your Subtitle",
    "class": "chartSubTitle"
  },
  "caption": {
    "enable": true,
    "text": "Figure 1. Write Your Caption text.",
    "class": "chartCaption"
  },
  "styles": {
    "classes": {
      "with-3d-shadow": true,
      "with-transitions": true,
      "gallery": false
    },
    "css": {
    }
  }
